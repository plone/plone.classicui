<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:zcml="http://namespaces.zope.org/zcml"
    >

  <configure zcml:condition="installed z3c.jbot">
    <include
        package="z3c.jbot"
        file="meta.zcml"
        />
    <browser:jbotDeprecated dictionary=".jbot.template_replacements" />
  </configure>

</configure>
